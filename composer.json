{
    "name": "virchow/virlumen",
    "description": "The Laravel Lumen Framework Modify By Virchow.",
    "keywords": ["framework", "laravel", "lumen", "virlumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "dingo/api": "^3.0",
        "godruoyi/dingo-api-helper": "^1.0",
        "laravel/lumen-framework": "^8.0",
        "palanik/lumen-cors": "^0.0.3",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "pre-install-cmd": "Installer\\Script::install",
        "pre-update-cmd": "Installer\\Script::install",
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
